{
  "name": "@cap-js/event-broker",
  "version": "1.0.0",
  "description": "CDS plugin providing integration to SAP Event Broker",
  "main": "cds-plugin.js",
  "peerDependencies": {
    "@sap/cds": "*"
  },
  "devDependencies": {
    "@sap/cds-mtxs": "*",
    "@sap/xssec": "*",
    "@cap-js/sqlite": "*",
    "@sap-cloud-sdk/resilience": "*"
  },
  "scripts": {
    "test": "jest"
  },
  "files": "cds-plugin.js",
  "keywords": [],
  "homepage": "https://cap.cloud.sap/",
  "author": "SAP SE (https://www.sap.com)",
  "license": "SEE LICENSE IN LICENSE",
  "cds": {
    "requires": {
      "kinds": {
        "event-broker": {
          "impl": "@cap-js/event-broker",
          "format": "cloudevents",
          "webhookPath": "/-/cds/event-broker/webhook",
          "vcap": {
            "label": "event-broker"
          }
        },
        "event-broker-internal": {
          "kind": "event-broker",
          "vcap": {
            "label": "eventmesh-sap2sap-internal"
          }
        }
      }
    }
  }
}
