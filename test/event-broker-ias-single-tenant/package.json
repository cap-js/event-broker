{
  "name": "event-broker-mock-app",
  "dependencies": {
    "@cap-js/event-broker": "*"
  },
  "cds": {
    "requires": {
      "messaging": {
        "kind": "event-broker",
        "sap-compliant-event": true,
        "outbox": false
      },
      "auth": {
        "kind": "ias",
        "credentials": {
          "certificate": "dummyCert",
          "key": "dummyKey"
        }
      },
      "ExtSrv": {
        "kind": "odata",
        "model": "./srv/external/extSrv.cds",
        "credentials": {}
      }
    }
  }
}
