{
  "name": "event-broker-mock-app",
  "dependencies": {
    "@cap-js/event-broker": "*"
  },
  "cds": {
    "requires": {
      "multitenancy": true,
      "messaging": {
        "kind": "event-broker-internal",
        "outboxed": false,
        "x509": {
          "cert": "$myCert",
          "pkey": "$myPkey"
        }
      },
      "ExtSrv": {
        "kind": "odata",
        "model": "./srv/external/extSrv.cds",
        "credentials": {}
      }
    }
  }
}
